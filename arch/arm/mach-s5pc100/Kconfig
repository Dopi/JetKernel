# Configuration options for the S5PC100 CPU

config CPU_S5PC100
	bool
	select CPU_S5PC100_INIT
	select CPU_S5PC100_CLOCK
	help
	  Enable S5PC100 CPU support

config MACH_SMDKC100
	bool "SMDKC100"
	select CPU_S5PC100
	help
	  Machine support for the Samsung SMDKC100

